            "aluminum ore config" {
                # Controls aluminum ore generation
                B:"Aluminum Ore Gen"=true

                # Aluminum ores per chunk
                # Min: 1
                # Max: 2147483647
                I:"Aluminum o/c"=20

                # Max Y Level where aluminum ore spawns
                # Must be greater than min
                # Min: 1
                # Max: 255
                I:"Max Y Aluminum"=50

                # Min Y Level where aluminum ore spawns
                # Must be less than max
                # Min: 0
                # Max: 254
                I:"Min Y Aluminum"=0
